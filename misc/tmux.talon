max:
  key(ctrl-space)
max split horizontally:
  key(ctrl-space)
  key(s)
max split vertically:
  key(ctrl-space)
  key(v)
max create window:
  key(ctrl-space)
  key(c)
max page up:
  key(ctrl-space)
  key(pageup)
max kill session:
  key(ctrl-space)
  insert(':kill-session\n')
max kill window:
  key(ctrl-space)
  insert(':kill-window\n')
max kill pane:
  key(ctrl-space)
  insert(':kill-pane\n')
max flip:
  key(ctrl-space)
  insert(":move-pane -t '.-'\n")
